(* 7. FOR-FOR-FOR loop *) 

let x := 0 in
read x;
let sum := 0 in
let i := 0 in
let j := 0 in
let k := 0 in
while (i < x) do (
   while (j < i) do (
     for k := 1 to 1 do (
       k := i
     );
     j := i + j
   );
   sum := sum + i;
   i := i + 1
); 
write(i);
write(j);
write(k);
write(sum)

(* input: 10, 10\n15\n8\n45\n *)
