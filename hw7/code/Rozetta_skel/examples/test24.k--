read x;
let sum := 0 in
let i := 0 in
let j := 0 in
let k := 0 in
while (i < x) do (
  for j := 1 to 1 do (
    for k := 1 to 1 do (
      unit
    )
  );
  sum := sum + i;
  i := i + 1
); 
