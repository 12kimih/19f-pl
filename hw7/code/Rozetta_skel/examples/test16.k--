(* 7. FOR-FOR-FOR loop *) 

let x := 0 in
read x;
let sum := 0 in
let i := 0 in
let j := 0 in
let k := 0 in
let p := 0 in
let q := 0 in
while (i < x) do (
   while (j < i) do (
     for k := 1 to x do (
       while (p < k) do (
         for q := 1 to p do (
           q := i - j + k - p
         );
         p := i + j + k
       );
       k := i + j
     );
     j := i + j
   );
   sum := sum + i;
   i := i + 1
); 
write(i);
write(j);
write(k);
write(p);
write(q);
write(sum)

